<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ossapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OssApp">

        <activity android:name=".FightingStylesActivity" />
        <activity android:name=".InformationActivity" />
        <activity android:name=".RulesActivity" />
        <activity android:name=".InputActivity" />
        <activity android:name=".AuthenticationActivity" />
        <activity android:name=".AvatarActivity"/>
        <activity android:name=".ChooseAvatarActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>